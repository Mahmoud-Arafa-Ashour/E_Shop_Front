{
  "name": "e-shop-frontend",
  "version": "1.0.0",
  "description": "Angular frontend for E-Shop application with product management and authentication",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
